[Unit]
Description=XEngine_StreamMedia CenterApp
After=network.target  
 
[Service]
Type=simple
WorkingDirectory=/home/ubuntu/XEngine_StreamMedia/XEngine_Release/
ExecStart=/home/ubuntu/XEngine_StreamMedia/XEngine_Release/XEngine_CenterApp -d 1
ExecStop=killall XEngine_CenterApp
 
[Install]
WantedBy=multi-user.target